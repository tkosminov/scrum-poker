{
  "name": "@scrum-pocker/back",
  "version": "1.0.0",
  "description": "",
  "author": "t.kosminov",
  "private": false,
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node --files -r tsconfig-paths/register src/main.ts",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:dev": "nodemon",
    "start:build": "node -r ts-node/register -r tsconfig-paths/register dist/main.js",
    "start:repl": "ts-node --files -r tsconfig-paths/register src/repl.ts",
    "start:build:repl": "node -r ts-node/register -r tsconfig-paths/register dist/repl.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint-fix": "npm run lint -- --fix",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "db:migration:create": "npm run typeorm:cli -- migration:create ./src/database/migrations/created",
    "db:migration:generate": "npm run typeorm:cli -- -d ./src/database/database-ormconfig.cli.ts migration:generate ./src/database/migrations/generated",
    "db:migration:run": "npm run typeorm:cli -- -d ./src/database/database-ormconfig.cli.ts migration:run",
    "db:migration:revert": "npm run typeorm:cli -- -d ./src/database/database-ormconfig.cli.ts migration:revert",
    "db:schema:sync": "npm run typeorm:cli -- -d ./src/database/database-ormconfig.cli.ts schema:sync"
  },
  "dependencies": {
    "@envelop/core": "^5.0.0",
    "@graphql-yoga/nestjs": "3.0.2",
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/microservices": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/terminus": "^10.2.0",
    "@nestjs/typeorm": "^10.0.1",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "dotenv-flow": "^4.0.1",
    "graphql": "^16.8.1",
    "graphql-redis-subscriptions": "^2.6.0",
    "graphql-scalars": "^1.22.4",
    "graphql-ws": "^5.14.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "nestjs-graphql-easy": "^3.1.4",
    "pg": "^8.11.3",
    "random-animal-name": "^0.1.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "typeorm-extension": "^3.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@types/body-parser": "^1.19.5",
    "@types/config": "^3.3.3",
    "@types/cookie-parser": "^1.4.6",
    "@types/dotenv-flow": "^3.3.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/nodemon": "^1.19.6",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "nodemon": "^3.0.2",
    "patch-package": "^8.0.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
